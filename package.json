    "@rollup/plugin-node-resolve": "^13.1.3",
    "babel-preset-solid": "^1.3.6",
    "colorette": "^2.0.16",
    "esbuild": "^0.14.23",
    "merge-anything": "^5.0.2",
    "rollup": "^2.68.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.5"
